<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
    <CommandSet xml:lang="en-us" Name="AdventureWorksCommandSet_en-us">
        <AppName> Standing Desk </AppName>
        <Example> Move to standing </Example>

        <Command Name="moveDesk">
            <Example> Move to standing </Example>
            <ListenFor RequireAppName="BeforeOrAfterPhrase"> [move] to {height} </ListenFor>
            <ListenFor RequireAppName="ExplicitlySpecified"> move [my] {builtin:AppName} to {height} </ListenFor>
            <Feedback> Moving desk to {height} </Feedback>
            <VoiceCommandService Target="StandingDeskVoiceService"/>
        </Command>

        <PhraseList Label="height">
            <Item>Sitting</Item>
            <Item>Standing</Item>
            <Item>Lowest</Item>
            <Item>Highest</Item>
        </PhraseList>
    </CommandSet>

